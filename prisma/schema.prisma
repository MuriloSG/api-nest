generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model AppUser {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
  phone    String @unique
  city     String
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt

  @@map("app_users")
}
